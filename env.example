# 서버 설정
PORT=3000 

# Firebase 설정
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_CLIENT_EMAIL=your-client-email@example.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour Private Key Here\n-----END PRIVATE KEY-----\n"

# 또는 서비스 계정 JSON 파일 경로
FIREBASE_SERVICE_ACCOUNT_PATH=./firebase-service-account.json

# iOS VoIP 푸시 알림 설정
IOS_BUNDLE_ID=com.example.yourapp # iOS 앱 번들 ID (예: com.company.appname)
APN_KEY_PATH=./AuthKey_XXXXXXXX.p8 # Apple Push Notification 인증 키 파일 경로
APN_KEY_ID=XXXXXXXX # Apple Developer 계정에서 발급받은 키 ID
APN_TEAM_ID=XXXXXXXXXX # Apple Developer 계정의 팀 ID